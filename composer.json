{
    "name": "aeviiq/value-object",
    "type": "library",
    "description": "A library that provides strict value objects in PHP.",
    "keywords": [
        "value-object"
    ],
    "homepage": "https://github.com/aeviiq/value-object",
    "license": "MIT",
    "authors": [
        {
            "name": "Aeviiq Value Object contributors",
            "homepage": "https://github.com/aeviiq/value-object/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aeviiq\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aeviiq\\ValueObject\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/validator": "^5.4|^6.0",
        "thecodingmachine/safe": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.59",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-strict-rules": "^0.12.5",
        "phpunit/phpunit": "^8.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "roave/security-advisories": "dev-master",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1",
        "vimeo/psalm": "^4.11"
    },
    "config": {
        "sort-packages": true
    }
}
