{
    "name": "aeviiq/value-object",
    "type": "library",
    "description": "A library that provides strict value objects in PHP.",
    "keywords": [
        "value-object"
    ],
    "homepage": "https://github.com/aeviiq/value-object",
    "license": "MIT",
    "authors": [
        {
            "name": "Aeviiq Value Object contributors",
            "homepage": "https://github.com/aeviiq/value-object/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aeviiq\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aeviiq\\ValueObject\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/validator": "^5.4|^6.0",
        "thecodingmachine/safe": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-phpunit": "^0.18.4",
        "roave/security-advisories": "dev-master",
        "thecodingmachine/phpstan-strict-rules": "^1.2",
        "vimeo/psalm": "^5.4"
    },
    "config": {
        "sort-packages": true
    }
}
